<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <declare-styleable name="ColorValueStateListItem" tools:ignore="ResourceName">
        <attr name="sd_color" format="color|reference"/>
        <attr name="android:alpha" />
    </declare-styleable>

    <declare-styleable name="NumberStateListItem" tools:ignore="ResourceName">
        <attr name="sd_number" format="integer|float|dimension|reference"/>
    </declare-styleable>

    <declare-styleable name="StyleStateListItem" tools:ignore="ResourceName">
        <attr name="sd_style" format="reference"/>
    </declare-styleable>
</resources>