<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <attr name="sd_rectSkeletonStyle" format="reference"/>
    <attr name="sd_textSkeletonStyle" format="reference"/>

    <declare-styleable name="RectSkeleton" tools:ignore="ResourceName" />
    <declare-styleable name="TextSkeleton" tools:ignore="ResourceName" >
        <attr name="sd_lineCount" format="integer" />
        <attr name="android:textAppearance" />
        <attr name="sd_shimmer" />
        <attr name="sd_shimmerDuration" />
        <attr name="sd_autoStart"/>
    </declare-styleable>
</resources>