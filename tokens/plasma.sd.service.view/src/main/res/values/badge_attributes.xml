<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<resources xmlns:tools="http://schemas.android.com/tools">
    <declare-styleable name="PlasmaBadgeColorState">
        <attr format="boolean" name="plasma_badge_state_default"/>
        <attr format="boolean" name="plasma_badge_state_accent"/>
        <attr format="boolean" name="plasma_badge_state_positive"/>
        <attr format="boolean" name="plasma_badge_state_negative"/>
        <attr format="boolean" name="plasma_badge_state_warning"/>
        <attr format="boolean" name="plasma_badge_state_clear"/>
        <attr format="boolean" name="plasma_badge_state_black"/>
        <attr format="boolean" name="plasma_badge_state_white"/>
    </declare-styleable>
    <declare-styleable name="Badge" tools:ignore="ResourceName">
        <attr format="enum" name="plasma_badgeColors">
            <enum name="state_default" value="0"/>
            <enum name="state_accent" value="1"/>
            <enum name="state_positive" value="2"/>
            <enum name="state_negative" value="3"/>
            <enum name="state_warning" value="4"/>
            <enum name="state_clear" value="5"/>
            <enum name="state_black" value="6"/>
            <enum name="state_white" value="7"/>
        </attr>
    </declare-styleable>
</resources>