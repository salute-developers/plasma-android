<?xml version="1.0" encoding="utf-8"?>

<com.sdds.uikit.NavigationBar xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    style="@style/Serv.Sdds.Components.NavigationBarMainPage.HasBackground.Shadow.Rounded"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_gravity="center"
    xmlns:android="http://schemas.android.com/apk/res/android"
    tools:ignore="ResourceName">

    <com.sdds.uikit.TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="title"
        app:layout_navigationBarContent="title"
        />

    <com.sdds.uikit.TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="description"
        app:layout_navigationBarContent="description"
        />

    <com.sdds.uikit.TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="content"
        app:layout_navigationBarContent="content"
        />

    <com.sdds.uikit.ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_plasma_24"
        app:layout_navigationBarContent="action_start"
        />
    </com.sdds.uikit.NavigationBar>

